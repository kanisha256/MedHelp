<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/tabletka"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.MedHelp"
        tools:targetApi="31">
        <activity

            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MedHelp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Home" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Profile" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Medecine" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Pacienty" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".AddPac" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".History" android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>